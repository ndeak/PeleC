# ------------------  INPUTS TO MAIN PROGRAM  -------------------
max_step = 10000
stop_time = 100.0

# PROBLEM SIZE & GEOMETRY
geometry.is_periodic =  1   0  1
geometry.coord_sys   =  0       # 0 => cart
geometry.prob_lo     =  0    0    0 
geometry.prob_hi     =  0.25  1.0  1.0
amr.n_cell           =  32 128 256
#amr.n_cell           =  256 256 256
#amr.n_cell           =  128 128 128
#amr.n_cell           =  16 16 16
#amr.n_cell           =  32 32 32
#amr.n_cell           =  64   80   64

# >>>>>>>>>>>>>  BC KEYWORDS <<<<<<<<<<<<<<<<<<<<<<
# Interior, UserBC, Symmetry, SlipWall, NoSlipWall
# >>>>>>>>>>>>>  BC KEYWORDS <<<<<<<<<<<<<<<<<<<<<<

pelec.lo_bc       =  "Interior" "SlipWall" "Interior"
pelec.hi_bc       =  "Interior" "SlipWall" "Interior"

# Problem setup
pelec.eb_boundary_T = 300.
pelec.eb_isothermal = 1
pelec.chem_integrator = 2
ode.implicit_solve = 1
#ode.iverbose = 4
ode.analytical_jacobian = 0
#cvode.solve_type = "dense"
cvode.solve_type = "dense"

# Plasma variables
pelec.ion_bc_type = 1
pelec.secondary_em_coef = 0.05

# WHICH PHYSICS
pelec.do_hydro = 1
pelec.do_mol = 1
pelec.do_react = 1
pelec.diffuse_temp = 1
pelec.diffuse_vel  = 1
pelec.diffuse_spec = 1
pelec.diffuse_enth = 1

# TIME STEP CONTROL
pelec.dt_cutoff      = 5.e-20  # level 0 timestep below which we halt
pelec.cfl            = 0.3     # cfl number for hyperbolic system
pelec.init_shrink    = 0.8    # scale back initial timestep
pelec.change_max     = 1.05     # maximum increase in dt over successive steps
pelec.max_dt = 1.0e-10

# DIAGNOSTICS & VERBOSITY
pelec.sum_interval   = 1       # timesteps between computing mass
pelec.v              = 1       # verbosity in PeleC cpp files
amr.v                = 1       # verbosity in Amr.cpp
#amr.grid_log         = grdlog  # name of grid logging file

# REFINEMENT / REGRIDDING
amr.max_level       = 0       # maximum level number allowed
amr.ref_ratio       = 2 2 2 2 # refinement ratio
amr.regrid_int      = 2       # how often to regrid
amr.blocking_factor = 16       # block factor in grid generation
amr.max_grid_size   = 256

tagging.max_ftracerr_lev = 1
tagging.ftracerr = 2.e-12
pelec.flame_trac_name = E

# CHECKPOINT FILES
amr.checkpoint_files_output = 0
amr.check_file      = chk      # root name of checkpoint file
amr.check_int       = -1       # number of timesteps between checkpoints

# PLOTFILES
amr.plot_files_output = 1
amr.plot_file       = plt
amr.plot_int        = 200
amr.derive_plot_vars=ALL
pelec.plot_massfrac = true

#extruded triangles lets the user create a maximum of 5 triangles 
#in 2D that will be extruded in the z direction
#make sure the coordinates are in anti-clockwise direction 
