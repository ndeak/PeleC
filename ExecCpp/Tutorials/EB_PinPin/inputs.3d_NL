pins.pin_0_tip = 5.5 5.0 5.5
pins.pin_1_tip = 5.5 6.0 5.5
pins.pin_0_c = -5
pins.pin_1_c = 5
pins.pin_0_dir = 1
pins.pin_1_dir = 1
